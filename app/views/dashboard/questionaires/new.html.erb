<div class="container-custom">
  <div class="row">
    <div class="col s4">
      <%= render partial: "shared/sidebar" %>
    </div>
    <div class="col s8">
      <h3>Create new template</h3>
      <div class="form-questionaire">
        <%= simple_form_for @questionaire, url: dashboard_templates_path, method: :post do |f| %>
        <%= f.input :title %>
        <%= f.input :instruction %>
        <button class="btn waves-effect waves-light" type="submit" name="commit">Submit
          <i class="material-icons right">send</i>
        </button>
        <% end %>
      </div>
    </div>
  </div>
</div>


